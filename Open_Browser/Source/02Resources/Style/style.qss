QTableView QTimeEdit:focus,
QTableView QDateTimeEdit:focus{
border-width:0px;
border-radius:0px;
}

/*---------------------------------------------------------------------------*/
/*  QTabWidget  表格视窗  */

QTabWidget::pane{
border:1px solid #67696B;
selection-background-color:#262829;
selection-color:#BEC0C2;
alternate-background-color:#262829;
gridline-color:#67696B;
background-color:rgb(100, 100, 100);
}


/*---------------------------------------------------------------------------*/
/*  QStackedWidget  切换窗口  */
QStackedWidget{

}

/*---------------------------------------------------------------------------*/
/*  QGraphicsView  图形窗口  */
QGraphicsView{
background-color:rgb(90, 90, 90);
border:0px
}



/*---------------------------------------------------------------------------*/
/* 重定义 （根据objectname）*/

/* 重定义  ui_aboutinfo */

QWidget#widget_title>QLabel#logo_label{
    background-image:url(:/Image/logo01.png);
}

/* 重定义  ui_formleft */
QWidget#widget_mode>QPushButton,
QWidget#widget_image>QPushButton{
font-size:12px;
text-align: center;
background-color:rgb(60, 60, 60);
border:none;
}

QWidget#widget_mode>QPushButton:hover,
QWidget#widget_image>QPushButton:hover{
background:qlineargradient(
spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #262829,stop:1 #262829);
}

/* 重定义  ui_formtitle */

QWidget#FormTitle>QFrame#formtitle_line{
background-color: rgb(255, 107, 107);
}

/* 重定义  ui_formmaskwidget */
QLabel#label_ai{
border-radius:3px;
background-color:rgb(115, 114, 120);
}



/* 重定义  all */

QLabel[border_no=true],
QStackedWidget[border_no=true]{
border: 0px solid gray;
}

QLabel[background_color="1"],
QPushButton[background_color="1"]{
background-color:rgb(60, 60, 60);
}

QLabel[background_color="2"],
QStackedWidget[background_color="2"],
QWidget[background_color="2"],
QPushButton[background_color="2"]{
background-color:rgb(70, 70, 70);
}

QWidget[background_color="3"]{
background-color:rgb(100, 100, 100);
}

QLabel[formshow="lab"]{
font:75 25px;color:#F0F0F0;
border:2px solid #AAAAAA;
background:#5A5A5A;
}

QLabel[formshow="lab"]:focus{
border:2px solid #00BB9E;
background:#555555;
}

QPushButton[CustomStyle="btnMenu"]{
border-radius:3px;
color:#BEC0C2;
padding:3px;
margin:0px;
background:none;
border-style:none;
}

QPushButton[CustomStyle="btnMenu"]:hover{
color:#FFFFFF;
margin:1px 1px 2px 1px;
background-color:rgba(51,127,209,230);
}

QPushButton[CustomStyle="switch"]:!checked{
image:url(:/Style/StyleResources/add_right.png);
width:10px;
height:10px;
padding:2px 5px 0px 0px;
border:1px solid transparent;
