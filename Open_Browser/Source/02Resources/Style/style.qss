padding:2px;
}

QPushButton[CustomStyle="switch"]:checked{
image:url(:/Style/StyleResources/add_left.png);
width:10px;
height:10px;
padding:2px 5px 0px 0px;
border:1px solid transparent;
border-radius:3px;
padding:2px;
}

