/*---------------------------------------------------------------------------*/
/*  QCalendarWidget  日历对话框  */

QCalendarWidget QToolButton::menu-indicator{
image:None;
}

QCalendarWidget QTableView{
border-width:0px;
}

/*---------------------------------------------------------------------------*/
/*  QCalendarWidget  表格视图  */

QTableView{
border:1px solid #67696B;
selection-background-color:#262829;
selection-color:#BEC0C2;
alternate-background-color:#262829;
gridline-color:#67696B;
}

QTableView{
background:#2E2F30;
}

QTableView::item:selected{
color:#BEC0C2;
background:qlineargradient(
spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #404244,stop:1 #404244);
}

QTableView::item:hover{
color:#BEC0C2;
background:qlineargradient(
spread:pad,x1:0,y1:0,x2:0,y2:1,stop:0 #262829,stop:1 #262829);
}

QTableView::item{
padding:1px;
margin:0px;
}

QTableView QLineEdit,
QTableView QComboBox,
QTableView QSpinBox,
QTableView QDoubleSpinBox,
QTableView QDateEdit,
QTableView QTimeEdit,
QTableView QDateTimeEdit{
border-width:0px;
border-radius:0px;
}

QTableView QLineEdit:focus,
QTableView QComboBox:focus,
QTableView QSpinBox:focus,
QTableView QDoubleSpinBox:focus,
QTableView QDateEdit:focus,
QTableView QTimeEdit:focus,
QTableView QDateTimeEdit:focus{
border-width:0px;
border-radius:0px;
}

/*---------------------------------------------------------------------------*/
/*  QTabWidget  表格视窗  */

QTabBar::tab {
    font-size:14px;
    background: rgb(51, 51, 55);
        border:1px solid rgb(72,72,76);
        color: rgb(111, 111, 111);
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
        min-width: 100px;
        min-height: 30px;
}
QTabBar::tab:hover {
    background: rgb(61, 61, 64);
}
QTabBar::tab:selected {
        color: rgb(233, 233, 233);
    background: rgb(90, 90, 96);
}


